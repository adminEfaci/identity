[flake8]
max-line-length = 88
extend-ignore = 
    E203,
    E501,
    W503,
    E231,
    ANN101,
    ANN102
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .mypy_cache,
    .pytest_cache,
    build,
    dist,
    *.egg-info,
    migrations/versions
per-file-ignores = 
    __init__.py:F401,F403
    tests/*:S101,S106
max-complexity = 10
import-order-style = google
application-import-names = domain,application,infrastructure
docstring-convention = google